<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Easy Gestion | abylsen STRA{% endblock %}</title>
        {# Stylesheets #}
        <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}" />
        <link rel="stylesheet" href="{{ asset('css/style.css') }}" /> 
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {# Navbar header #}
        {% block nav %}{% endblock %}
        <div class="navbar navbar-default">
            <div class="container">
                <a href="{{ path('easygestion_homepage') }}" class="navbar-brand">ABYLSEN</a>
            </div>
        </div>
           
        {# Body #}
        {% block body %}{% endblock %}
        
        {# Navbar footer #}
  
        
        <script src="{{ asset('js/jquery-1.10.0.min.js') }}"></script>
        <script src="{{ asset('js/moment-with-locales.js') }}"></script>
        <script src="https://cdn.datatables.net/v/bs/jszip-2.5.0/pdfmake-0.1.18/dt-1.10.13/af-2.1.3/b-1.2.4/b-flash-1.2.4/b-html5-1.2.4/b-print-1.2.4/cr-1.3.2/fc-3.2.2/fh-3.1.2/kt-2.2.0/r-2.1.1/rr-1.2.0/sc-1.4.2/se-1.2.0/datatables.min.js"></script>
        <script src="{{ asset('js/bootstrap.js') }}"></script>
        
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        
        <script src="{{ asset('bundles/sgdatatables/js/pipeline.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
